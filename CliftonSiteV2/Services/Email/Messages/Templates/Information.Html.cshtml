@using RazorLight
@using CliftonSiteV2.Models
@inherits TemplatePage<EmailMessage>

@{
    this.Layout = "Messages.Templates._Layout.cshtml";
    this.ViewBag.Title = "Website Information Request";
}

<table border="0" cellpadding="0" cellspacing="0" width="100%">

    <tr>
        <td align="center" style="padding: 0 0 8px 0; color: #1565c0; font-family: 'Roboto', Helvetica, Arial, sans-serif; font-size: 28px; font-weight: 400; line-height: 32px;">
            Information request from the website
        </td>
    </tr>
    <tr>
        <td align="center" style="padding: 0 0 16px 0; color: #424242; font-family: 'Roboto', Helvetica, Arial, sans-serif; font-size: 16px; font-weight: normal; line-height: 28px;">
            We have received the following query from the website.
        </td>
    </tr>
    <tr>
        <td>
            @{ await IncludeAsync("Messages.Templates.Partials._ContactUsDetails.Html.cshtml", Model); }
        </td>
    </tr>
</table>
